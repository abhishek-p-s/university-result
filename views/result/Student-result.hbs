      <td>{{this.grade5}}</td>
    </tr>
    <tr>
      <th scope="row">{{this.courseCode6}}</th>
      <td>{{this.course_title6}}</td>
      <td>{{this.marks6}}</td>
      <td>{{this.total_marks6}}</td>
      <td>{{this.grade6}}</td>
    </tr>
    {{/each}}
    
  </tbody>
</table>