      <td>{{this.course_title3}}</td>
      <td>{{this.marks3}}</td>
      <td>{{this.total_marks3}}</td>
      <td>{{this.grade3}}</td>
    </tr>
    <tr>
      <th scope="row">{{this.courseCode4}}</th>
      <td>{{this.course_title4}}</td>
      <td>{{this.marks4}}</td>
