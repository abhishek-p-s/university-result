
<h1 class="text-center p-3">University of calicut</h1>

<table class="table">
  <thead>
    {{#each marks}}
    <tr>
      <th scope="col">Name:{{this.name}}</th>
      <th scope="col">Register number:{{this.register}}</th>
      <th scope="col"></th>
      <th scope="col"></th>
       <th scope="col"></th>
    </tr>
    {{/each}}
    <tr>
      <th scope="col">Course code</th>
      <th scope="col">Course title</th>
      <th scope="col">Marks</th>
      <th scope="col">Total Marks</th>
       <th scope="col">Grade</th>
    </tr>
  </thead>
  <tbody>
  
    {{#each marks}}

    
    <tr>
      <th scope="row">{{this.courseCode1}}</th>
